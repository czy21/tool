  "scripts": {
    "start": "craco start",
    "build": "craco build"
  },
  "eslintConfig": {
    "rules": {
      "import/no-anonymous-default-export": [
        "error",
        {
          "allowArray": true,
          "allowArrowFunction": true,
          "allowAnonymousClass": true,
          "allowAnonymousFunction": true,
          "allowCallExpression": true,
          "allowNew": true,
          "allowLiteral": true,
          "allowObject": true
        }
      ]
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
